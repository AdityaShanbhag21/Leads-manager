{
  "manifest_version": 3,
  "name": "Leads tracker",
  "version": "1.0",
  "action": {
    "default_popup": "index.html",
    "default_icon": "logo.png"
  },
  "permissions": [
    "tabs",
    "storage"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background.js"
  }
}
